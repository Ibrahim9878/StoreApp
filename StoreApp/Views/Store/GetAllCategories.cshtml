@model List<GetAllCategoriesViewModels>


<div class="col-sm-6 mb-3 mb-sm-0">
	@foreach (var item in @Model)
	{
		<div class="card m-3">
			<div class="card-body">
				<h5 class="card-title">@item.Name</h5>
				<div class="flex flex-row">
 					<a href="/Store/DeleteCategory/@item.Id" class="btn btn-danger">Delete</a>
					<a href="/Store/UpdateCategory/@item.Id" class="btn btn-warning">Update</a>
				</div>
			</div>
		</div>
	}
</div>